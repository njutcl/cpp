
#include <iostream>
using namespace std;

class Complex {
public:
    Complex() {
        real = 0;
        imag = 0;
    }

    Complex (double r, double i) {
        real = r;
        imag = i;
    }
    Complex operator+(Complex &c2);
   operator double(){
   	return real;
   }
   friend  Complex operator+(int i,Complex &c);
   friend  Complex operator+(Complex &c,int i);
    void display();
private:
    double real;
 double dl; 
    double imag;
};
